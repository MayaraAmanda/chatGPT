import multiprocessing

# Função que será executada em paralelo
def processar_dado(dado):
    # Código de processamento para cada dado
    # ...
    resultado = dado * 2
    # ...

    return resultado

if __name__ == '__main__':
    # Dados a serem processados
    dados = [1, 2, 3, 4, 5]

    # Número de processos paralelos
    num_processos = multiprocessing.cpu_count()

    # Criação do pool de processos
    pool = multiprocessing.Pool(processes=num_processos)

    # Mapeamento dos dados para os processos em paralelo
    resultados = pool.map(processar_dado, dados)

    # Fechamento do pool de processos
    pool.close()
    pool.join()

    # Exibir os resultados
    print(resultados)
