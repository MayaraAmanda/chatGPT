CREATE TABLE tabela1 (
    coluna1 TEXT,
    coluna2 INTEGER,
    coluna3 REAL
);

CREATE TABLE tabela2 (
    coluna1 TEXT,
    coluna2 INTEGER,
    coluna3 REAL
);

CREATE TABLE tabela3 (
    coluna1 TEXT,
    coluna2 INTEGER,
    coluna3 REAL
);







import sqlite3

# Conectar ao banco de dados SQLite (criará um novo arquivo de banco de dados se não existir)
conn = sqlite3.connect('mydatabase.db')
cursor = conn.cursor()

# Criar tabela1
cursor.execute('''
    CREATE TABLE IF NOT EXISTS tabela1 (
        coluna1 TEXT,
        coluna2 INTEGER,
        coluna3 REAL
    )
''')

# Criar tabela2
cursor.execute('''
    CREATE TABLE IF NOT EXISTS tabela2 (
        coluna1 TEXT,
        coluna2 INTEGER,
        coluna3 REAL
    )
''')

# Criar tabela3
cursor.execute('''
    CREATE TABLE IF NOT EXISTS tabela3 (
        coluna1 TEXT,
        coluna2 INTEGER,
        coluna3 REAL
    )
''')

# Dados em memória
dados_tabela1 = [
    ('valor1', 1, 1.1),
    ('valor2', 2, 2.2),
    ('valor3', 3, 3.3)
]

dados_tabela2 = [
    ('valor4', 4, 4.4),
    ('valor5', 5, 5.5),
    ('valor6', 6, 6.6)
]

dados_tabela3 = [
    ('valor7', 7, 7.7),
    ('valor8', 8, 8.8),
    ('valor9', 9, 9.9)
]

# Inserir dados na tabela1
cursor.executemany('INSERT INTO tabela1 VALUES (?, ?, ?)', dados_tabela1)

# Inserir dados na tabela2
cursor.executemany('INSERT INTO tabela2 VALUES (?, ?, ?)', dados_tabela2)

# Inserir dados na tabela3
cursor.executemany('INSERT INTO tabela3 VALUES (?, ?, ?)', dados_tabela3)

# Confirmar as alterações no banco de dados
conn.commit()

# Fechar a conexão com o banco de dados
conn.close()
